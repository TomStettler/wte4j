<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FormPanel action="j_security_check" method="post">
		<g:FlowPanel>
			<g:Label text="Login"/>
			<g:TextBox name="j_username" />
			<g:Label text="Password"/>
			<g:PasswordTextBox name="j_password"/>
			<g:SubmitButton text="Submit" />
			<g:ResetButton text="Reset" />
		</g:FlowPanel>
	</g:FormPanel>
</ui:UiBinder> 